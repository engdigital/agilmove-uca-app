{
  "name": "agilmove-uca-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "pwa:setup": "node scripts/generate-icons.js",
    "pwa:build": "npm run pwa:setup && npm run build",
    "pwa:validate": "node scripts/check-images.js && echo 'Manifest disponível em: http://localhost:3000/manifest.json'",
    "icons:generate": "node scripts/generate-icons.js",
    "icons:android": "node scripts/setup-android-icons.js",
    "icons:setup": "npm run icons:generate && npm run icons:android && npm run build && npx cap sync",
    "icons:check": "node scripts/check-icons-status.js",
    "icons:verify": "echo 'Verificando ícones PWA...' && dir public\\icon-*.png 2>nul || echo 'Ícones não encontrados. Execute: npm run pwa:setup'",
    "screenshots:create": "node scripts/create-wide-screenshot.js",
    "cap:sync": "npx cap sync",
    "cap:android": "npx cap open android",
    "cap:build": "npm run build && npx cap sync",
    "cap:dev": "npm run cap:build && npm run cap:android",
    "cap:bundle": "npm run build && npx cap sync && cd android && gradlew bundleRelease",
    "cap:apk": "npm run build && npx cap sync && cd android && gradlew assembleRelease",
    "security:check": "node security-check.js",
    "security:headers": "node scripts/monitoring/check-headers.js",
    "security:report": "node scripts/monitoring/generate-security-report.js",
    "security:performance": "node scripts/monitoring/performance-monitor.js",
    "security:full": "npm run security:check && npm run security:headers && npm run security:report",
    "monitoring:start": "npm run security:performance",
    "monitoring:auto": "node scripts/monitoring/auto-monitor.js",
    "monitoring:continuous": "node scripts/monitoring/auto-monitor.js continuous",
    "monitoring:status": "node scripts/monitoring/auto-monitor.js status",
    "monitoring:headers": "npm run security:headers",
    "monitoring:report": "npm run security:report",
    "monitoring:dashboard": "echo 'Dashboard disponível em: http://localhost:3000/security-dashboard'",
    "store:check": "node scripts/store-readiness-check.js",
    "store:autocheck": "node scripts/monitoring/store-readiness-autocheck.js",
    "store:prepare": "npm run pwa:build && npm run store:autocheck",
    "store:validate": "npm run build && npm run store:autocheck && echo 'Projeto pronto para submissão às stores!'",
    "signing:validate": "validate-google-play-signing.bat",
    "signing:backup": "backup-keystores.bat",
    "signing:upload-key": "setup-upload-key.bat"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.2",
    "@capacitor/cli": "^7.4.2",
    "@capacitor/core": "^7.4.2",
    "@capacitor/ios": "^7.4.2",
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-accordion": "1.2.2",
    "@radix-ui/react-alert-dialog": "1.1.4",
    "@radix-ui/react-aspect-ratio": "1.1.1",
    "@radix-ui/react-avatar": "1.1.2",
    "@radix-ui/react-checkbox": "1.1.3",
    "@radix-ui/react-collapsible": "1.1.2",
    "@radix-ui/react-context-menu": "2.2.4",
    "@radix-ui/react-dialog": "1.1.4",
    "@radix-ui/react-dropdown-menu": "2.1.4",
    "@radix-ui/react-hover-card": "1.1.4",
    "@radix-ui/react-label": "2.1.1",
    "@radix-ui/react-menubar": "1.1.4",
    "@radix-ui/react-navigation-menu": "1.2.3",
    "@radix-ui/react-popover": "1.1.4",
    "@radix-ui/react-progress": "1.1.1",
    "@radix-ui/react-radio-group": "1.2.2",
    "@radix-ui/react-scroll-area": "1.2.2",
    "@radix-ui/react-select": "2.1.4",
    "@radix-ui/react-separator": "1.1.1",
    "@radix-ui/react-slider": "1.2.2",
    "@radix-ui/react-slot": "1.1.1",
    "@radix-ui/react-switch": "1.1.2",
    "@radix-ui/react-tabs": "1.1.2",
    "@radix-ui/react-toast": "latest",
    "@radix-ui/react-toggle": "1.1.1",
    "@radix-ui/react-toggle-group": "1.1.1",
    "@radix-ui/react-tooltip": "1.1.6",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.4",
    "date-fns": "4.1.0",
    "dexie": "latest",
    "dexie-react-hooks": "latest",
    "embla-carousel-react": "8.5.1",
    "input-otp": "1.4.1",
    "lucide-react": "^0.454.0",
    "next": "14.2.16",
    "next-themes": "^0.4.4",
    "react": "^18",
    "react-day-picker": "8.10.1",
    "react-dom": "^18",
    "react-hook-form": "^7.54.1",
    "react-is": "^19.1.0",
    "react-resizable-panels": "^2.1.7",
    "recharts": "latest",
    "sonner": "^1.7.1",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.6",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "postcss": "^8.5",
    "sharp": "^0.34.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5"
  }
}
